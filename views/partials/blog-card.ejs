<div class="card mb-3">
  <div class="row">
    <div class="col-md-3">
      <img
        src="/static/images/<%=blogimage %>"
        class="card-img-top"
        alt="Blog Resmi"
      />
    </div>
    <div class="col-md-9">
      <div class="card-body">
        <h5 class="card-title"><%= title %></h5>
        <p class="card-text"><%= description %></p>
      </div>
    </div>
  </div>
</div>
